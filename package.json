    "popper.js": "^1.12",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
      "@coreui/coreui": "^2.1.12",
      "@coreui/icons": "^0.3.0",
      "@fortawesome/fontawesome-free": "^5.9.0",
      "laravel-echo": "^1.5.4",
      "perfect-scrollbar": "^1.4.0",
      "pusher-js": "^5.0.0",
      "quill": "^1.3.6",
      "simple-line-icons": "^2.4.1"
